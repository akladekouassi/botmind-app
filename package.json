{
    "name": "botmind-app",
    "version": "0.0.0",
    "license": "MIT",
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "------GENERATE-ENVIRONMENT-FILES-CMDs------": "",
        "config:generate:envFiles": "ts-node ./scripts/setenv.ts",
        "------RUN-FRONT-BACK-TOGATHER-CMDs------": "",
        "dev": "concurrently \"yarn start:api\" \"yarn start:front\"",
        "------ALL-FRONTEND-RELATIVES-CMDs------": "",
        "start:front": "yarn config:generate:envFiles -- --environment=dev && ng serve",
        "build:frontend": "yarn config:generate:envFiles -- --environment=prod && ng build --prod frontend",
        "------ALL-BACKEND-RELATIVES-CMDs------": "",
        "postinstall": "yarn build:frontend",
        "start:api": "yarn config:generate:envFiles -- --environment=dev && nx serve backend",
        "build:backend": "yarn config:generate:envFiles -- --environment=prod && nx build backend --configuration=production",
        "start": "node dist/apps/frontend/main",
        "build": "concurrently \"yarn build:frontend\" \"yarn build:backend\"",
        "------ALL-TEST-AND-LINTNG-RELATIVES-CMDs------": "",
        "test": "ng test",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "------ALL-CMSs-RELATIVES-TO-NX------": "",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "------ALL-CMSs-RELATIVES-TO-FORMATING------": "",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "format:all": "prettier --write \"src/**/*.+(js|jsx|md|ts|tsx|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
        "------ALL-CMSs-RELATIVES-TO-COMMITS------": "",
        "commit": "git-cz",
        "------OTHERS-CMSs--------": "",
        "": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "nx": "nx",
        "ng": "nx"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^11.2.12",
        "@angular/cdk": "^11.2.11",
        "@angular/common": "^11.2.0",
        "@angular/compiler": "^11.2.0",
        "@angular/core": "^11.2.0",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "^11.2.0",
        "@angular/http": "^7.2.16",
        "@angular/material": "^11.2.11",
        "@angular/platform-browser": "^11.2.0",
        "@angular/platform-browser-dynamic": "^11.2.0",
        "@angular/router": "^11.2.0",
        "@nrwl/angular": "12.0.8",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.10",
        "@types/express-serve-static-core": "^4.17.19",
        "@types/express-session": "^1.17.3",
        "@types/morgan": "^1.9.2",
        "@types/passport": "^1.0.6",
        "@types/shortid": "^0.0.29",
        "@types/uuid": "^8.3.0",
        "bcryptjs": "^2.4.3",
        "concurrently": "^6.0.2",
        "connect-mongo": "^4.4.1",
        "cors": "^2.8.5",
        "date-fns": "^2.21.1",
        "express": "4.17.1",
        "express-paginate": "^1.0.2",
        "express-session": "^1.17.1",
        "express-validator": "^6.10.1",
        "jsonwebtoken": "^8.5.1",
        "lowdb": "^1.0.0",
        "mongoose": "^5.12.7",
        "morgan": "^1.10.0",
        "ngx-toastr": "^13.2.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "path": "^0.12.7",
        "rxjs": "~6.6.3",
        "shortid": "^2.2.16",
        "tslib": "^2.0.0",
        "uuid": "^8.3.2",
        "zone.js": "^0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.0",
        "@angular/cli": "~11.2.0",
        "@angular/compiler-cli": "^11.2.0",
        "@angular/language-service": "^11.2.0",
        "@nrwl/cli": "12.0.8",
        "@nrwl/cypress": "12.0.8",
        "@nrwl/express": "12.0.8",
        "@nrwl/jest": "12.0.8",
        "@nrwl/node": "12.0.8",
        "@nrwl/tao": "12.0.8",
        "@nrwl/workspace": "12.0.8",
        "@types/jest": "26.0.8",
        "@types/node": "14.14.33",
        "codelyzer": "^6.0.0",
        "cypress": "^6.0.1",
        "cz-conventional-changelog": "3.3.0",
        "dotenv": "8.2.0",
        "jest": "26.2.2",
        "jest-preset-angular": "8.3.2",
        "prettier": "2.2.1",
        "ts-jest": "26.4.0",
        "ts-node": "~9.1.1",
        "tslint": "~6.1.0",
        "typescript": "~4.1.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./apps/*.{js,jsx,ts,tsx}": [
            "npx prettier --write",
            "eslint apps/*.tsx --fix-dry-run"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}